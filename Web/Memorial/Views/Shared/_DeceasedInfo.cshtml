@model Memorial.Core.Dtos.DeceasedDto

<div class="form-group row">
    <div class="col-md-3">
        IC
    </div>
    <div class="col-md-6">
        @Model.IC
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">
        Name
    </div>
    <div class="col-md-3">
        @Model.Name
    </div>
    <div class="col-md-3">
        Name2
    </div>
    <div class="col-md-3">
        @Model.Name2
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">
        Age
    </div>
    <div class="col-md-3">
        @Model.Age
    </div>
    <div class="col-md-3">
        Gender
    </div>
    <div class="col-md-3">
        @Model.GenderType.Name
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">
        Marital
    </div>
    <div class="col-md-3">
        @Model.MaritalType.Name
    </div>
    <div class="col-md-3">
        Religion
    </div>
    <div class="col-md-3">
        @Model.ReligionType.Name
    </div>
</div>
<div class="form-group row">
    <div class="col-md-3">
        Address
    </div>
    <div class="col-md-3">
        @Model.Address
    </div>
    <div class="col-md-3">
        Nationality
    </div>
    <div class="col-md-3">
        @Model.NationalityType.Name
    </div>
</div>
<div class="form-group row">
    <div class="col-md-2">
        Death Date
    </div>
    <div class="col-md-2">
        @Model.DeathDate.ToString("yyyy-MM-dd HH:mm")
    </div>
    <div class="col-md-2">
        Death Registration Centre
    </div>
    <div class="col-md-2">
        @Model.DeathRegistrationCentre
    </div>
    <div class="col-md-2">
        Death Certificate
    </div>
    <div class="col-md-2">
        @Model.DeathCertificate
    </div>
</div>
<div class="form-group row">
    <div class="col-md-2">
        Burial Certificate
    </div>
    <div class="col-md-2">
        @Model.BurialCertificate
    </div>
    <div class="col-md-2">
        Import Permit Number
    </div>
    <div class="col-md-2">
        @Model.ImportPermitNumber
    </div>
    <div class="col-md-2">
        Remark
    </div>
    <div class="col-md-2">
        @Model.Remark
    </div>
</div>
