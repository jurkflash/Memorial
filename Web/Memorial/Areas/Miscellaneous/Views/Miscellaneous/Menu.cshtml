@model IEnumerable<Memorial.Core.Dtos.RecentDto>
@{
    ViewBag.Title = "Menu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Menu</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>@Html.Raw(Resources.Mix.AFBR)</th>
            <th>@Html.Raw(Resources.Mix.ApplicantBR)</th>
            <th>@Html.Raw(Resources.Mix.ItemBR)</th>
            <th>@Html.Raw(Resources.Mix.MiscellaneousBR)</th>
            <th>@Html.Raw(Resources.Mix.CreateDateBR)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var recent in Model)
        {
            <tr>
                <td>@Html.ActionLink(recent.Code, "Index", recent.LinkController, new { itemId = recent.ItemId, filter = recent.Code, area = recent.LinkArea, applicantId = 0 }, null)</td>
                <td>@recent.ApplicantName</td>
                <td>@recent.ItemName</td>
                <td>@recent.Text1</td>
                <td>@recent.CreateDate</td>
            </tr>
        }
    </tbody>
</table>
