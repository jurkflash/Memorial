@model Memorial.ViewModels.CemeteryTransactionsInfoViewModel
@{
    ViewBag.Title = "Info";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Info</h2>

@{Html.RenderAction("PartialViewInfo", "Applicants", new { id = Model.ApplicantId, area = "Applicant" });}

<div class="row">
    <div class="col-md-3">
        Amount
    </div>
    <div class="col-md-3">
        @Model.CemeteryTransactionDto.Price
    </div>
</div>
<div class="row">
    <div class="col-md-3">
        Remark
    </div>
    <div class="col-md-3">
        @Model.CemeteryTransactionDto.Remark
    </div>
</div>
